{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {},
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["jsx-a11y", "import", "prettier"],
  "rules": {
    "import/extensions": [2, "always"]
  }
}
